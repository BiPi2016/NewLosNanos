<nav class="mainNav">        
        <%if(topMenu) {%>
            <ul class="<%= sideNav ? 'sideNavList' : 'topNavList'%>">
                <%topMenu.forEach( (tm, indexTop) => {%>
                    <li class="<%= sideNav ? 'sideNavList__item' : 'topNavList__item'%>">
                        <%if(sideNav) {%>
                            <label for="checkSideNavTopCat<%=indexTop%>" ><span class="sideNavPlusContainer"><%=tm.name%></span></label>
                        <%} else {%>
                            <p class="topMenuOptionContainer">
                                <a href="<%=tm.endPoint%>" class="topNav__link"><%=tm.name%></a>
                                <label for="checkTopCat<%=indexTop%>" class="topCatLabel"><span class="plusContainer">+</span> </label>
                            </p>
                        <%}%>
                        
                        <input type="checkbox" name="" id="checkTopCat<%=indexTop%>" class="checkTopCat">
                        <input type="checkbox" name="" id="<%= sideNav ? 'checkSideNavTopCat' + indexTop : '' %>" class="checkSideNavTopCat">
                        <ul class="<%= sideNav ? 'sideNav__subCatList' : 'topNav__subCatList'%>" >
                            <% tm.subCats.forEach( (s, indexSub) => {%>
                                <li class="<%= sideNav ? 'sideNav__subCatList__item' : 'topNav__subCatList__item'%>">
                                    <%if(sideNav) {%>
                                        <% if(s.subCats) {%>
                                            <label for="sideNavChk<%=indexTop%><%=indexSub%>" class="sideNavSubMenuLabel"><span class="sideNavPlusContainer sideNavPlusContainerSubMenu"><%=s.name%></span></label>
                                        <%}%>
                                    <%} else {%>
                                        <p class="subMenuOptionContainer">
                                            <a href="<%=s.endPoint%>" class="subCat__link"><%= s.name %></a>
                                            <% if(s.subCats) {%>
                                                <label for="<%=indexTop%><%=indexSub%>" class="subMenuLabel"><span class="plusContainer">+</span></label>
                                            <%}%>
                                        </p>
                                    <%}%>                                       
                                    <div class="divider"></div>                                
                                    <% if(s.subCats) {%>
                                        <input type="checkbox" id="<%= sideNav ? 'sideNavChk' +indexTop + indexSub : '' %>" class="chkSideNavSubCat">
                                        <input type="checkbox" class="checkSubCat" id="<%=indexTop%><%=indexSub%>"> 
                                        <!-- The value of id in the checkbox and for in the label need to use two indexs <%=indexTop%> and <%indexSub%>,
                                        one from the outer loop and the other from the inner loop.
                                        It is similar to accessing elements of a multidimensional array. -->
                                        <ul class="<%= sideNav ? 'sideNavInnerMost__list' : 'innermost__list'%>">
                                            <% s.subCats.sort() %>
                                            <% s.subCats.forEach( menu => {%>
                                                <li class="<%= sideNav ? 'sideNavInnerMost__list__item' : 'innermost__list__item'%>">
                                                    <a href="/?gender=<%=tm.name%>&category=<%=s.name%>&type=<%=menu%>" class="innermost__link"><%=menu%></a>
                                                </li>
                                            <%})%>
                                        </ul>
                                    <%}%>
                                </li>
                            <%})%>
                        </ul> 
                    </li>
                <%})%>
            </ul>
        <%}%>
    </nav>